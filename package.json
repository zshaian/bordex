  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "prettier:fix": "prettier --write .",
    "prepare": "husky",
    "test": "jest",
    "rollup": "rollup -c --bundleConfigAsCjs",
